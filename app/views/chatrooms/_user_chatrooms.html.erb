<h5>ChatRooms</h5>
<ul class="chatrooms" data-user-id="<%= current_user.id %>">
  <% current_user.chatrooms.each do |chatroom| %>
    <li data-behaviour='user-chatrooms', data-chatroom-id="<%= chatroom.id %>">
      <%= link_to chatroom.titleize_name, chatroom_path(chatroom) %>
      <span></span>
    </li>
  <% end %>
</ul>
